    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@types/glob": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "front-matter": "^4.0.2",
    "glob": "^8.0.1",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rehype": "^12.0.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
